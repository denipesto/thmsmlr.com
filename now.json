{
  "version": 2,
  "builds": [
        { "src": "package.json", "use": "@now/next" }
  ],
  "routes": [
    {
      "src": "/distributed-tracing-zero-to-one",
      "status": 308,
      "headers": { "Location": "/blog/distributed-tracing-zero-to-one" }
    },

    {
      "src": "/distributed-tracing-zero-to-one/",
      "status": 308,
      "headers": { "Location": "/blog/distributed-tracing-zero-to-one" }
    },

    {
      "src": "/images/(.*)",
      "headers": { "cache-control": "s-maxage=604800, max-age=604800, public" },
      "dest": "/images/$1"
    }
  ]
}
